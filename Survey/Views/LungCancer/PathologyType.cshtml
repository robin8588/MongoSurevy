
@{
    ViewBag.Title = "PathologyType";
}

<h2>病理类型</h2>
<div class="row">
    <div class="col-md-12">
        <form action="@Url.Action("Period")" method="post">
            
            <div class="radio">
                <label>
                    <input type="radio" name="PathologyType" value="小细胞肺癌" />
                    小细胞肺癌
                </label>
            </div>
            <span>非小细胞肺癌</span>
            <div class="col-md-offset-1">
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="鳞状上皮细胞癌" />
                        鳞状上皮细胞癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="大细胞癌" />
                        大细胞癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="腺癌" />
                        腺癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="腺鳞癌" />
                        腺鳞癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="内瘤样癌" />
                        内瘤样癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="类癌" />
                        类癌
                    </label>
                </div>
                <div class="radio">
                    <label>
                        <input type="radio" name="PathologyType" value="其他病例类型" />
                        其他病例类型
                    </label>
                </div>
            </div>
            <div class="radio">
                <label>
                    <input type="radio" name="PathologyType" value="未知" />
                    未知
                </label>
            </div>
            <button type="submit" class="btn btn-default">下一步</button>
        </form>
    </div>
</div>
